<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:startColor="#666560"
                android:centerColor="#666560"
                android:centerY="0.75"
                android:endColor="#666560"
                android:angle="270" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#666560"
                    android:centerColor="#666560"
                    android:centerY="0.75"
                    android:endColor="#666560"
                    android:angle="270" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#1fe23c"
                    android:centerColor="#1fe23c"
                    android:centerY="0.75"
                    android:endColor="#1fe23c"
                    android:angle="270" />
            </shape>
        </clip>
    </item>

</layer-list>

