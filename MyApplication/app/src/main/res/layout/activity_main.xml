                <ImageButton
                    android:id="@+id/bottom_4"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitCenter"
                    app:layout_constraintBottom_toBottomOf="@+id/bottom_1"
                    app:layout_constraintEnd_toStartOf="@+id/bottom_5"
                    app:layout_constraintStart_toEndOf="@+id/bottom_3"
                    app:layout_constraintTop_toTopOf="@+id/bottom_1"
                    app:srcCompat="@drawable/bottom_bar_4"
                    tools:ignore="MissingConstraints" />
            </androidx.constraintlayout.widget.ConstraintLayout>
